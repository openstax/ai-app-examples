{
  "name": "ai-app-experiment-examples",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/setup.js && vite",
    "ci:typecheck": "tsc -b --noEmit",
    "ci:spelling": "git ls-files -- . | cspell lint -c ./cspell.json --quiet --file-list stdin",
